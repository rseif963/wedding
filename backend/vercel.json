{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "config/**",
          "models/**",
          "middleware/**",
          "routes/**",
          "utils/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/api/not-found"
    }
  ]
}
